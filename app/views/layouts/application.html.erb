<!DOCTYPE html>
<html>
<head>
  <title>BootstrapTutorial</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if notice %>
  <p class="notice"><%= notice %></p>
<% end %>

<% if alert %>
  <p class="alert"><%= alert %></p>
<% end %>
<%= yield %>

</body>
</html>
